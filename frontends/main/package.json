{
  "name": "eosio-card-game",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eosjs": "20.0.0-beta3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-modal": "^3.5.1",
    "react-redux": "^5.0.7",
    "react-scripts": "^2.0.0",
    "redux": "^4.0.0",
    "replace": "^1.1.0",
    "text-encoding": "^0.7.0"
  },
  "scripts": {
    "postbuild": "replace 'static' './static' build/static/css/*",
    "start": "source $PWD/.env;react-scripts start",
    "build": "source $PWD/.env;react-scripts build && sed 's/\\\"\\/static/\\\"\\.\\/static/g' -i build/index.html",
    "test": "source $PWD/.env;react-scripts test --env=jsdom",
    "eject": "source $PWD/.env;react-scripts eject"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
